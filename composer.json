{
    "name": "fnayou/slave",
    "type": "library",
    "description": "minimalistic cli tool made with symfony/console and without silex",
    "keywords": [
        "php",
        "console",
        "command",
        "cli",
        "notation"
    ],
    "homepage": "https://github.com/fnayou/slave",
    "license": "MIT",
    "authors": [
        {
            "name": "Aymen FNAYOU",
            "email": "fnayou.aymen@gmail.com",
            "homepage": "http://aymen-fnayou.com",
            "role": "Developer"
        }
    ],
    "require": {
        "fnayou/dotted": "^1.0",
        "incenteev/composer-parameter-handler": "^2.1",
        "monolog/monolog": "^1.23",
        "pimple/pimple": "^3.0",
        "symfony/config": "^3.3",
        "symfony/console": "^3.3",
        "symfony/event-dispatcher": "^3.3",
        "symfony/finder": "^3.3",
        "symfony/yaml": "^3.3"
    },
    "require-dev": {
        "escapestudios/symfony2-coding-standard": "^2.10",
        "fnayou/standards": "^1.2",
        "friendsofphp/php-cs-fixer": "^2.3",
        "phpunit/phpunit": "^5.1",
        "symfony/var-dumper": "^3.3"
    },
    "autoload" : {
        "psr-4" : {
            "Fnayou\\Slave\\": "src/Slave/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fnayou\\Slave\\Tests\\": "tests/"
        }
    },
    "extra" : {
        "branch-alias" : {
            "dev-master" : "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
